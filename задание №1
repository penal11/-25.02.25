import pandas as pd

df_excel = pd.read_excel('Excel.xlsx') # Читаем Excel файл

line = int(input("введите строку которы хотите вывести от 1 до 10"))



value = df_excel.loc[line,"X"] # Используя метод loc выбираем нужную строчку столбца X
print(value)  # Выводим строчку столбца



# так как в столбцах X и Y одиноковая информация выводил сточки из столбца X


